<config idBy="refid">
!-- when idBy is set to refid, it implies meaningless identifiers, codes, are used for the ref:ID and 
	they uniquely identify concepts -->

		<!-- tier non-specific information below -->
<panel>
   <panel-name>def</panel-name>	
				<!-- use: term, def, complex -->
				<!-- any property that is not associated with any of 
					these three panels is displayed in the "simple properties"
					panel of the "properties" sub-tab -->
   <panel-label>Definition</panel-label>   			<!-- NC/NT -->
   <owl-property>
	  <name root="ComplexDefinition">P97</name>
	  <table>	
		<field> 
			<element>def-definition</element>		<!-- special value: $language -->
			<label>Definition Text</label>		<!-- NC -->
			<type>text-paragraph</type>			<!-- special values: $username, $date -->
		</field>
		<field>
			<element>xml:lang</element>
			<label>Lang</label>			<!-- NT -->
			<enumeration default="en">$language</enumeration>
		</field>
	
	  </table>	  
	  <table type="binary">					<!-- ? -->
	  	<field> 
			<element>Definition_Review_Date</element>
			<label>Review Date</label>		<!-- NC -->
			<value>$date</value>			
		</field>
		<field>
			<element>def-source</element>
			<label>Definition Source</label>	<!-- NC -->
			<enumeration default="NCI">			  
			  <value>CADSR</value>
			  <value>CDISC</value>
			  <value>CRCH</value>
			  <value>CTCAE</value>
			  <value>DICOM</value>
			  <value>FDA</value>
			  <value>ICD03</value>
			  <value>NCI</value>
			  <value>NCI-GLOSS</value>
			  <value>SPL</value>			  
			</enumeration>
		</field>
		<field>
			<element>attr</element>
			<label>Attribution</label>		<!-- NC -->
			<type>text-line</type>
		</field>
		<field>
			<element>Definition_Reviewer_Name</element>
			<label>Reviewer Name</label>		<!-- NC -->
			<value>$username</value>
		</field>
	  </table>
   </owl-property>
</panel>

<panel>
   <panel-name>term</panel-name>
   <panel-label>Terms</panel-label>		   		<!-- NC/NT -->
   <owl-property>
	<name root="ComplexTerm">P90</name>
	  <table>
		<field> 
			<element>term-name</element>
			<label>Term</label>			<!-- NC -->
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>term-group</element>
			<label>Term Type</label>		<!-- NC -->
			<enumeration default="SY">
			  <value>PT</value>
			  <value>AB</value>
			  <value>AD</value>
			  <value>AQ</value>
			  <value>AQS</value>
			  <value>BR</value>
			  <value>CA2</value>
			  <value>CA3</value>
			  <value>CN</value>
			  <value>CNU</value>
			  <value>DN</value>
			  <value>FB</value>
			  <value>HD</value>
			  <value>SN</value>
			  <value>SY</value>
			</enumeration>
		</field>
		<field>
			<element>term-source</element>
			<label>Term Source</label>		<!-- NT -->
			<enumeration default="NCI">
			  <value>BioCarta</value>
			  <value>CADSR</value>
			  <value>CDC</value>
			  <value>CDISC</value>
			  <value>CRCH</value>
			  <value>CTCAE</value>
			  <value>CTRM</value>
			  <value>DCP</value>
			  <value>DICOM</value>
			  <value>DTP</value>
			  <value>FDA</value>
			  <value>HL7</value>
			  <value>ICH</value>
			  <value>JAX</value>
			  <value>KEGG</value>
			  <value>MTH</value>
			  <value>NCI-GLOSS</value>
			  <value>NCI</value>
			  <value>SEER</value>
			  <value>UCUM</value>
			</enumeration>
		</field>
		<field>
			<element>source-code</element>
			<label>Code</label>			<!-- NC -->
			<type>text-line</type>
		</field>
		<field>
			<element>xml:lang</element>
			<label>Lang</label>			<!-- NT -->
			<enumeration default="en">$language</enumeration>
		</field>
	  </table>
   </owl-property>
</panel>

<panel>
   <panel-name>complex</panel-name>
   <panel-label>Complex Properties</panel-label>  		<!-- NT --> 
   <owl-property>
	<name root="ComplexDefinition">P325</name>
	  <table>
		<field> 
			<element>def-definition</element>
			<label>Definition Text</label>		<!-- NC -->
			<type>text-paragraph</type>			
		</field>
		<field>
			<element>xml:lang</element>
			<label>Lang</label>			<!-- NT -->
			<enumeration default="en">$language</enumeration>
		</field>
	  </table>
	  <table type="binary">
	  	<field> 
			<element>Definition_Review_Date</element>
			<label>Review Date</label>		<!-- NC -->
			<value>$date</value>			
		</field>
		<field>
			<element>def-source</element>
			<label>Definition Source</label>	<!-- NC -->
			<enumeration default="NCI">
			  <value>CADSR</value>
			  <value>CDISC</value>
			  <value>CRCH</value>
			  <value>CTCAE</value>
			  <value>DICOM</value>
			  <value>FDA</value>
			  <value>ICD03</value>
			  <value>NCI</value>
			  <value>NCI-GLOSS</value>
			  <value>SPL</value>
			</enumeration>
		</field>
		<field>
			<element>attr</element>
			<label>Attribution</label>		<!-- NC -->
			<type>text-line</type>
		</field>
		<field>
			<element>Definition_Reviewer_Name</element>
			<label>Reviewer Name</label>		<!-- NC -->
			<value>$username</value>
		</field>
	  </table>
   </owl-property>
   <owl-property>
	<name root="ComplexGOAnnotation">P211</name>
	  <table>
		<field> 
			<element>go-term</element>
			<label>term</label>			<!-- NC -->
			<type>text-paragraph</type>			
		</field>
		<field>
			<element>xml:lang</element>
			<label>Lang</label>			<!-- NT -->
			<enumeration default="en">$language</enumeration>
		</field>
	
	  </table>	  
	  <table type="binary">
		<field>
			<element>go-id</element>
			<label>GO Identifier</label>		<!-- NC -->
			<type>text-line</type>
		</field>
		<field>
			<element>go-evi</element>
			<label>Evidence Code</label>		<!-- NC -->
			<type>text-line</type>
		</field>
	  	<field> 
			<element>source-date</element>
			<label>Assignment Date</label>		<!-- NC -->
			<value>$date</value>			
		</field>
		<field>
			<element>go-source</element>
			<label>Assignment Source</label>	<!-- NC -->
			 <enumeration default="NCI">		<!-- enumeration?  format? -->
			  <value>NCI</value>
			  <value>NCBI</value>
			  <value>CGAP</value>
			<type>text-line</type>			
		</field>
	  </table>
   </owl-property>
</panel>

