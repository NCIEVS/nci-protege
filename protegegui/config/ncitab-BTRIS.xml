
<config idBy="code" useRules="nci">
<!-- when idBy is set to code, it implies meaningful identifiers, names, are used for the ref:ID and 
	another property, in this case codes is used to uniquely identify concepts -->
	
<panel>
   <panel-name>def</panel-name>	
				<!-- use: term, def, complex -->
				<!-- any property that is not associated with any of 
					these three panels is displayed in the "simple properties"
					panel of the "properties" sub-tab -->
   <panel-label>Definition</panel-label>   
   <owl-property>
	  <name root="ComplexDefinition">DEFINITION</name>
	  <table>	
		<field> 
			<element>def-definition</element>	<!-- special value: $language -->
			<label>Definition Text</label>
			<type>text-paragraph</type>			<!-- special values: $username, $date -->
		</field>
		<field>	
			<element>xml:lang</element>
			<label>Lang</label>
			<enumeration default="en">$language</enumeration>
		</field>	
	  </table>	  
	  <table type="binary">
	  	<field> 
			<element>Definition_Review_Date</element>
			<label>Review Date</label>
			<value>$date</value>			
		</field>
		<field>
			<element>def-source</element>
			<label>Definition Source</label>
			<enumeration default="RED">
			  <value>BRIDG</value>
			  <value>CADSR</value>
			  <value>CDISC</value>
			  <value>CRCH</value>
			  <value>CTCAE</value>
			  <value>DICOM</value>
			  <value>FDA</value>
			  <value>ICD03</value>
			  <value>NCI</value>
			  <value>NCI-GLOSS</value>
			  <value>RED</value>
			  <value>SPL</value>
			 </enumeration>
		</field>
		<field>
		  <element>attr</element>
          <label>Attribution</label>
          <type>text-line</type>
          </field>
        <field>
			<element>Definition_Reviewer_Name</element>
			<label>Reviewer Name</label>
			<value>$username</value>
		</field>
	  </table>
   </owl-property>
</panel>

<panel>
   <panel-name>term</panel-name>
   <panel-label>Terms</panel-label>   
   <owl-property>
	<name root="ComplexTerm">FULL_SYN</name>
	  <table>
		<field> 
			<element required="true">term-name</element>
			<label>Term</label>
			<type>text-paragraph</type>			
		</field>
	  	<field> 
			<element required="true">term-group</element>
			<label>Term Type</label>
			<enumeration default="SY">
			  <value>PT</value>
			  <value>AB</value>
			  <value>AD</value>
			  <value>AQ</value>
			  <value>AQS</value>
			  <value>BR</value>
			  <value>CA2</value>
			  <value>CA3</value>
			  <value>CN</value>
			  <value>CNU</value>
			  <value>DN</value>
			  <value>FB</value>
			  <value>HD</value>
			  <value>SN</value>
			  <value>SY</value>
			</enumeration>
		</field>
		<field>
			<element required="true">term-source</element>
			<label>Term Source</label>
			<enumeration default="RED">
			  <value>CADSR</value>
			  <value>CDC</value>
			  <value>CDISC</value>
			  <value>CRCH</value>
			  <value>CTCAE</value>
			  <value>DICOM</value>
			  <value>FDA</value>
			  <value>HL7</value>
			  <value>ICH</value>
			  <value>NCI</value>
			  <value>NCI-GLOSS</value>
			  <value>NICHD</value>
			  <value>NCPDP</value>
			  <value>RED</value>
			  <value>UCUM</value>
			</enumeration>
		</field>
		<field>
			<element>source-code</element>
			<label>Code</label>
			<type>text-line</type>
		</field>		
		<field>
			<element>xml:lang</element>
			<label>Lang</label>
			<enumeration default="en">$language</enumeration>
		</field>
		<field>
			<element>subsource-name</element>
			<label>Subsource</label>
			<type>text-line</type>
		</field> 
	  </table>
   </owl-property>
</panel>

<panel>
   <panel-name>complex</panel-name>
   <panel-label>Complex Properties</panel-label>
   <owl-property>
	<name root="ComplexDefinition">ALT_DEFINITION</name>
	  <table>
		<field> 
			<element>def-definition</element>
			<label>Definition Text</label>		<!-- NC -->
			<type>text-paragraph</type>			
		</field>
		<field>
			<element>xml:lang</element>
			<label>Lang</label>			<!-- NT -->
			<enumeration default="en">$language</enumeration>
		</field>
	
	  </table>	  
	  <table type="binary">
	  	<field> 
			<element>Definition_Review_Date</element>
			<label>Review Date</label>		<!-- NC -->
			<value>$date</value>			
		</field>
		<field>
			<element>def-source</element>
			<label>Definition Source</label>	<!-- NC -->
			<enumeration default="RED">
			  <value>BRIDG</value>
			  <value>CADSR</value>
			  <value>CDISC</value>
			  <value>CRCH</value>
			  <value>CTCAE</value>
			  <value>DICOM</value>
			  <value>FDA</value>
			  <value>ICD03</value>
			  <value>NCI</value>
			  <value>NCI-GLOSS</value>
			  <value>RED</value>
			  <value>SPL</value>
			</enumeration>
		</field>
		<field>
			<element>attr</element>
			<label>Attribution</label>		<!-- NC -->
			<type>text-line</type>
		</field>
		<field>
			<element>Definition_Reviewer_Name</element>
			<label>Reviewer Name</label>		<!-- NC -->
			<value>$username</value>
		</field>
	  </table>
   </owl-property>
   <owl-property>
	<name root="ComplexGOAnnotation">GO_Annotation</name>
	  <table>
		<field> 
			<element>go-term</element>
			<label>term</label>
			<type>text-paragraph</type>			
		</field>
		<field>
			<element>xml:lang</element>
			<label>Lang</label>
			<enumeration default="en">$language</enumeration>
		</field>

	  </table>	  
	  <table type="binary">
		<field>
			<element>go-id</element>
			<label>GO Identifier</label>
			<type>text-line</type>
		</field>
		<field>
			<element>go-evi</element>
			<label>Evidence Code</label>
			<type>text-line</type>
		</field>
	  	<field> 
			<element>source-date</element>
			<label>Assignment Date</label>
			<value>$date</value>			
		</field>
		<field>
			<element>go-source</element>
			<label>Assignment Source</label>
			<enumeration default="NCI">
			  <value>NCI</value>
			  <value>NCBI</value>
			  <value>CGAP</value>
			</enumeration>
		</field>
	  </table>
   </owl-property>

   <owl-property>
	<name root="ComplexStatus">Lab_Test_Status</name>
	  <table>
		<field> 
			<element required="true">lts-value</element>
			<label>term</label>
			<type>text-paragraph</type>			
		</field>
		<field>
			<element>xml:lang</element>
			<label>Lang</label>
			<enumeration default="en">$language</enumeration>
		</field>
	  </table>	  
	  <table type="binary">
	  	<field> 
			<element>Status_Date</element>
			<label>Status Date</label>
			<value>$date</value>			
		</field>
	  </table>
   </owl-property>


   <owl-property>   
	<name root="ComplexCode">Contributing_Source_Local_Code</name>
	  <table>
		<field> 
			<element required="true">cslc-value</element>
			<label>Contributing Source Local Code</label>
			<type>text-paragraph</type>			
		</field>
		<field>
			<element>xml:lang</element>
			<label>Language</label>
			<enumeration default="en">$language</enumeration>
		</field>
	  </table>	  
	  <table type="binary">
	  	<field> 
			<element>Use_Code</element>
			<label>Use Code</label>
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>Use_Source</element>
			<label>Use Source</label>
			<type>text-line</type>			
		</field>
	  </table>
   </owl-property>


   <owl-property>
	<name root="ComplexSource">Reference_Source</name>
	  <table>
		<field> 
			<element>rs-value</element>
			<label>term</label>
			<type>text-paragraph</type>			
		</field>
		<field>
			<element>xml:lang</element>
			<label>Lang</label>
			<enumeration default="en">$language</enumeration>
		</field>

	  </table>	  
	  <table type="binary">
	  	<field> 
			<element>HL7_Code</element>
			<label>HL7_Code</label>
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>HL7_Name</element>
			<label>HL7_Name</label>
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>ICD-91_Code</element>
			<label>ICD-91_Code</label>
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>ICD-9_Code</element>
			<label>ICD-9_Code</label>
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>LOINC_Code</element>
			<label>LOINC_Code</label>
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>MedDRA_Code</element>
			<label>MedDRA_Code</label>
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>SNOMED_Code</element>
			<label>SNOMED_Code</label>
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>SNOMED_PT</element>
			<label>SNOMED_PT</label>
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>UMLS_CUI</element>
			<label>UMLS_CUI</label>
			<type>text-line</type>			
		</field>
	  	<field> 
			<element>UMLS_Semantic_Type</element>
			<label>UMLS_Semantic_Type</label>
			<type>text-line</type>			
		</field>
	  </table>
   </owl-property>

</panel>

<menu-disable>
	<menu-bar>Project</menu-bar>
	<menu-item>Metrics...</menu-item>
</menu-disable>

<menu-disable>
	<menu-bar>Project</menu-bar>
